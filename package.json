{
  "name": "codingacademy-psdhtml",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --parallel babel browser styles",
    "browser": "browser-sync start --server --files *.html, build/*.js, dist/*.css",
    "styles": "node-sass scss/style.scss -o dist --watch",
    "build": "rm -rf build && copyfiles assets/* index.html assets/fonts/**/* dist/* build",
    "babel": "babel --presets es2015 --watch src/main.js -o build/main.bundle.js"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "browser-sync": "^2.18.1",
    "copyfiles": "^1.0.0",
    "node-sass": "^3.13.0",
    "npm-run-all": "^3.1.1"
  },
  "dependencies": {
    "normalize.css": "^5.0.0",
    "vue-material": "^0.6.3"
  }
}
